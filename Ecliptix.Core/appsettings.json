{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId", "WithEnvironmentName" ],
    "Properties": {
      "Application": "Ecliptix"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "EcliptixDb": "Server=postgres-db;Port=5432;Database=Luxave;User Id=postgres;Password=1111;",
    "PostgresUrl": "postgresql://postgres:1111@localhost:5432/ecliptix"
  },
  "AppSettings": {
    "LogFilePath": "app.log"
  },
  "GrpcServer": {
    "Address": "127.0.0.1",
    "Port": 5051
  },
  "AWS": {
    "AccessKeyId": "AKIAX3AP54OH6YYM3SZ3",
    "SecretAccessKey": "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",
    "Region": "eu-central-1"
  }
}